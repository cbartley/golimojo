<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
    
        <em:id>golimojo@golimojo.com</em:id>
        <em:name>Golimojo Wikipedia Extension</em:name>
        <em:version>1.002</em:version>
        <em:description>Automatically add links to Wikipedia articles</em:description>
        <em:creator>Golimojo</em:creator>
        <!-- optional items -->
        <em:homepageURL>http://www.golimojo.com/</em:homepageURL>

                <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>1.0</em:minVersion>
                <em:maxVersion>100.0</em:maxVersion>
            </Description>
        </em:targetApplication>

                <!-- This is not needed for Firefox 1.1 and later. Only include this 
                     if you want to make your extension compatible with older versions -->
        <em:file>
            <Description about="urn:mozilla:extension:file:golimojo.jar">
                <em:package>content/</em:package>
                <!-- optional items -->
                <em:locale>locale/en-US/</em:locale>
            </Description>
        </em:file>
    </Description>

</RDF>
