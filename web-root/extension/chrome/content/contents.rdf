    xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

    <!-- list all packages -->
    <RDF:Seq about="urn:mozilla:package:root">
        <RDF:li resource="urn:mozilla:package:golimojo"/>
    </RDF:Seq>

    <!-- package information -->
    <RDF:Description about="urn:mozilla:package:golimojo"
        chrome:displayName="Golimojo"
